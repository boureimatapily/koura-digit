{
  "expo": {
    "name": "koura-digit",
    "slug": "koura-digit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.koura.koura"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.koura.koura"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
